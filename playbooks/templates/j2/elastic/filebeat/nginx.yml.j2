# Module: nginx
# Docs: https://www.elastic.co/guide/en/beats/filebeat/7.17/filebeat-module-nginx.html

- module: nginx
  # Access logs
  access:
    enabled: true
    var.paths:
      - "/var/log/nginx/*-access.log*"

  # Error logs
  error:
    enabled: true
    var.paths:
      - "/var/log/nginx/*-error.log*"
